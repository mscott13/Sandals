             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="Sandals.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="AccentColor">#016FB9</Color>
            <Color x:Key="ColorPrimaryDark"></Color>
            <Color x:Key="ColorPrimary"></Color>
            <Color x:Key="TitleHeader">#020402</Color>
            <Color x:Key="TitleSub">#646464</Color>
            <Color x:Key="StandardTextColor">#252525</Color>
            <Color x:Key="PageBackground">#f6f6f6</Color>
        </ResourceDictionary>
